{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install",
		"gen": "npm run gen-zod-client && npm run gen-mocks",
		"gen-zod-client": "openapi-zod-client ./meta/api.yml -t ./src/api/template.hbs -o ./src/api/api.client.ts",
		"gen-mocks": "msw-auto-mock ./meta/api.yml -z ./src/api/api.client.ts -o ./src/mocks/handlers.ts --base-url https://o2gather.ptass.org/api/v1"
	},
	"devDependencies": {
		"@anatine/zod-mock": "^3.12.0",
		"@faker-js/faker": "^8.0.2",
		"@playwright/test": "^1.34.3",
		"@sentry/node": "^7.54.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.1",
		"@testing-library/svelte": "^3.2.2",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.30.0",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"lint-staged": "^13.2.2",
		"msw": "^1.2.1",
		"msw-auto-mock": "github:LaiJunBin/msw-auto-mock",
		"mswx": "^0.4.0",
		"openapi-zod-client": "^1.7.0",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-loading-spinners": "^0.3.4",
		"sveltekit-superforms": "^0.7.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"uuid": "^9.0.0",
		"vite": "^4.3.9",
		"vitest": "^0.29.8",
		"zod": "^3.21.4"
	},
	"type": "module",
	"msw": {
		"workerDirectory": "static"
	},
	"dependencies": {
		"@zodios/core": "^10.9.0",
		"axios": "^1.4.0"
	}
}
